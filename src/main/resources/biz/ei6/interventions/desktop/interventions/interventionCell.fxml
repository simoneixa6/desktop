<fx:root type="javafx.scene.control.ListCell" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml">
    <graphic>
        <HBox alignment="CENTER_LEFT" maxHeight="-Infinity" maxWidth="500" minWidth="-Infinity" prefWidth="300" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
           <children>
              <ImageView fx:id="status" fitHeight="13.0" fitWidth="13.0" pickOnBounds="true" preserveRatio="true">
                 <HBox.margin>
                    <Insets right="10.0" />
                 </HBox.margin>
              </ImageView>
              <Label fx:id="lblIntervention" text="Intervention 1 - Nom Client" />
           </children>
        </HBox>
    </graphic>
</fx:root>